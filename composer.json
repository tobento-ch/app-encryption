{
    "name": "tobento/app-encryption",
    "type": "library",
    "description": "App encryption support.",
    "keywords": [
        "tobento",
        "package",
        "app",
        "encryption"
    ],
    "homepage": "https://www.tobento.ch",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Strub",
            "email": "info@tobento.ch",
            "homepage": "https://www.tobento.ch"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "tobento/app": "^1.0",
        "tobento/app-migration": "^1.0",
        "tobento/service-config": "^1.0.4",
        "tobento/service-encryption": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.0",
        "tobento/service-filesystem": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tobento\\App\\Encryption\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tobento\\App\\Encryption\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --colors=always",
        "psalm": "vendor/bin/psalm"
    }
}